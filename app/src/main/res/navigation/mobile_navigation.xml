<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_home">

    <!-- home.ui.storetypes.StoreTypesFragment -->
    <fragment
        android:id="@+id/nav_home"
        android:name="com.corptia.bringero.view.home.ui.storetypes.StoreTypesFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_store_types"
        app:popUpToInclusive="false"
        />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.corptia.bringero.view.gallaryStore.GallaryFragment"
        android:label="@string/gallery"
        tools:layout="@layout/fragment_gallary" />
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.corptia.bringero.view.Setting.main.SettingFragment"
        android:label="@string/setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_nav_settings_to_changePasswordFragment2"
            app:destination="@id/changePasswordFragment2"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"/>
        <action
            android:id="@+id/action_nav_settings_to_languageFragment2"
            app:destination="@id/languageFragment2"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"/>
        <action
            android:id="@+id/action_nav_settings_to_contactUsFragment2"
            app:destination="@id/contactUsFragment2"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"/>
        <action
            android:id="@+id/action_nav_settings_to_editProfileFragment2"
            app:destination="@id/editProfileFragment2"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"/>
    </fragment>
    <fragment
        android:id="@+id/contactUsFragment2"
        android:name="com.corptia.bringero.view.Setting.contactUs.ContactUsFragment"
        android:label="@string/contact_us"
        tools:layout="@layout/fragment_contact_us" />
    <fragment
        android:id="@+id/languageFragment2"
        android:name="com.corptia.bringero.view.Setting.language.LanguageFragment"
        android:label="@string/language"
        tools:layout="@layout/fragment_language" />
    <fragment
        android:id="@+id/editProfileFragment2"
        android:name="com.corptia.bringero.view.Setting.editProfile.EditProfileFragment"
        android:label="@string/edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/changePasswordFragment2"
        android:name="com.corptia.bringero.view.Setting.changePassword.ChangePasswordFragment"
        android:label="@string/change_password"
        tools:layout="@layout/fragment_change_password" />
    <fragment
        android:id="@+id/nav_pricing"
        android:name="com.corptia.bringero.view.pricing.PricingProductFragment"
        android:label="@string/product_pricing"
        tools:layout="@layout/fragment_pricing_product" />
    <fragment
        android:id="@+id/nav_cart"
        android:name="com.corptia.bringero.view.cart.CartFragment"
        android:label="@string/cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_nav_cart_to_checkOutFragment"
            app:destination="@id/checkOutFragment"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popUpTo="@+id/nav_cart"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/checkOutFragment"
        android:name="com.corptia.bringero.view.cart.checkOut.CheckOutFragment"
        android:label="@string/check_out"
        tools:layout="@layout/fragment_check_out" >
    </fragment>
    <fragment
        android:id="@+id/nav_order"
        android:name="com.corptia.bringero.view.order.OrderFragment"
        android:label="@string/orders"
        tools:layout="@layout/fragment_order" />

</navigation>