        <attr name="text_value" format="string" />
        <attr name="Text_color" format="color" />
        <attr name="Text_size" format="float" />
        <attr name="right_icon_src" format="integer" />
        <attr name="left_icon_src" format="integer" />
        <attr name="right_icon_x_position" format="float" />
        <attr name="text_x_position" format="float" />
        <attr name="text_y_position" format="float" />
        <attr name="left_icon_x_position" format="float" />
        <attr name="right_icon_y_position" format="float" />
        <attr name="left_icon_y_position" format="float" />
        <attr name="is_left_icon" format="boolean" />
        <attr name="is_right_icon" format="boolean" />
        <attr name="is_text" format="boolean" />
        <attr name="is_finish" format="boolean" />
        <attr name="is_notification_number" format="boolean" />

    </declare-styleable>

</resources>